#include<stdio.h>
int main() {
	int mid,final,q1,q2,lab,sit;
	//assign value to variables
	mid=75 ;final=85 ;
	q1=40 ;q2=45 ;
	lab=16 ;sit=15 ;
	//display score
	printf("%10s=%8d\n%10s=%8d\n","Mid",mid,"Final",final);
	printf("%10s=%8d\n%10s=%8d\n","Q1",q1,"Q2",q2);
	printf("%10s=%8d\n%10s=%8d\n","Lab",lab,"Sit",sit);

}
